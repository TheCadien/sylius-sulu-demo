<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>products</key>

    <properties>
        <property name="id" visibility="no" translation="sulu_admin.id">
            <field-name>id</field-name>
            <entity-name>App\Entity\Product</entity-name>
        </property>

        <property name="code" visibility="always" searchability="yes" translation="app.code">
            <field-name>code</field-name>
            <entity-name>App\Entity\Product</entity-name>
        </property>

        <property name="name" visibility="always" searchability="yes" translation="sulu_admin.name">
            <field-name>name</field-name>
            <entity-name>App\Entity\Product</entity-name>
        </property>

        <property name="price" visibility="yes" translation="app.price">
            <field-name>price</field-name>
            <entity-name>App\Entity\Product</entity-name>
        </property>
    </properties>
</list>
